<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE kcfg SYSTEM
    "http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
<kcfg>
  <group name="PlayPause" />
  <group name="Stop" />
  <group name="SkipNext" />
  <group name="SkipPrevious" />
  <group name="SeekForward" />
  <group name="SeekBackward" />
  <group name="Seek">
    <entry name="position" type="double">
      <label>The position to seek to, as percentage in the stream.</label>
    </entry>
  </group>
</kcfg>
